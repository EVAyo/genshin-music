{
  "name": "genshin-music",
  "engines": {
    "node": ">=16.17.0"
  },
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@insertish/zangodb": "^1.0.12-nomemo",
    "@pixi/graphics-smooth": "^1.1.0",
    "@pixi/react": "^7.1.1",
    "@spotify/basic-pitch": "^1.0.1",
    "@tauri-apps/api": "^1.5.0",
    "@tonejs/midi": "^2.0.28",
    "audiobuffer-to-wav": "^1.0.0",
    "color": "^4.2.3",
    "eslint-config-next": "^14.1.4",
    "fflate": "^0.8.1",
    "fontfaceobserver": "^2.3.0",
    "fuzzy-search": "^3.2.1",
    "is-mobile": "^4.0.0",
    "lodash.clonedeep": "^4.5.0",
    "mobx": "^6.10.2",
    "next": "^13.5.4",
    "pepjs": "^0.5.3",
    "pixi.js": "^7.2.4",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "semver": "^7.5.4",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "worker-timers": "^7.0.76"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "npx serve@latest build",
    "preview:sky": "npx serve@latest build/skyMusic",
    "preview:genshin": "npx serve@latest build/genshinMusic",
    "tauri": "tauri",
    "build-tauri-deployment": "cross-env NEXT_PUBLIC_IS_TAURI=true node ./scripts/buildAndPrepareTauriRelease.js true",
    "build-tauri-deployment:no-env": "cross-env NEXT_PUBLIC_IS_TAURI=true node ./scripts/buildAndPrepareTauriRelease.js false",
    "build-tauri:sky": "cross-env NEXT_PUBLIC_IS_TAURI=true env-cmd npm run tauri build --config ./src-tauri/tauri-sky.conf.json",
    "build-tauri:sky-no-env": "cross-env NEXT_PUBLIC_IS_TAURI=true npm run tauri build --config ./src-tauri/tauri-sky.conf.json",
    "build-tauri:genshin": "cross-env NEXT_PUBLIC_IS_TAURI=true env-cmd npm run tauri build --config ./src-tauri/tauri-genshin.conf.json",
    "build-tauri:genshin-no-env": "cross-env NEXT_PUBLIC_IS_TAURI=true npm run tauri build --config ./src-tauri/tauri-genshin.conf.json",
    "build-tauri:all": "cross-env NEXT_PUBLIC_IS_TAURI=true npm run build-tauri:sky && npm run build-tauri:genshin",
    "build:sky": "node ./scripts/buildApp.js Sky",
    "build:sky:analyze": "cross-env ANALYZE=true npm run build:sky",
    "build:genshin:analyze": "cross-env ANALYZE=true npm run build:genshin",
    "build:genshin": "node ./scripts/buildApp.js Genshin",
    "build:sky-no-root": "node ./scripts/buildApp.js Sky false",
    "build:genshin-no-root": "node ./scripts/buildApp.js Genshin false",
    "build:all": "node ./scripts/buildApp.js All",
    "build:all-no-root": "node ./scripts/buildApp.js All false",
    "dev-tauri:sky": "npm run tauri dev --config ./src-tauri/tauri-sky.conf.json",
    "dev-tauri:genshin": "npm run tauri dev --config ./src-tauri/tauri-genshin.conf.json",
    "dev:sky": "node ./scripts/startApp.js Sky",
    "dev:genshin": "node ./scripts/startApp.js Genshin"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@next/bundle-analyzer": "^13.5.4",
    "@tauri-apps/cli": "^1.5.1",
    "@types/audiobuffer-to-wav": "^1.0.2",
    "@types/color": "^3.0.4",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/fuzzy-search": "^2.1.2",
    "@types/jest": "^29.5.5",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/next-pwa": "^5.6.5",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/semver": "^7.5.3",
    "@types/webmidi": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cli-color": "^2.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "next-pwa": "^5.6.0",
    "sass": "^1.68.0",
    "typescript": "^5.4.5",
    "url-join": "^5.0.0"
  }
}
